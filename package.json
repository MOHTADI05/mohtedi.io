{
  "name": "mohtedi-portfolio",
  "version": "1.0.0",
  "description": "Modern portfolio website for Mohtadi Marmouri with parallax animations",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p 8000 -o",
    "dev": "npx live-server --port=8000",
    "minify-css": "cleancss -o css/style.min.css css/style.css",
    "minify-js": "uglifyjs js/main.js -o js/main.min.js -c -m",
    "minify": "npm run minify-css && npm run minify-js",
    "optimize-images": "node scripts/optimize-images.js"
  },
  "keywords": [
    "portfolio",
    "parallax",
    "dark-theme",
    "mohtadi-marmouri",
    "data-science",
    "ai",
    "fintech"
  ],
  "author": {
    "name": "Mohtadi Marmouri",
    "email": "mohtadimarmouri@gmail.com",
    "url": "https://github.com/mohtadi"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohtadi/mohtedi.io.git"
  },
  "homepage": "https://mohtadi.github.io/mohtedi.io",
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {}
}

